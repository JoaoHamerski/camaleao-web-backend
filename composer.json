{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "doctrine/dbal": "^2.0",
        "dyrynda/laravel-cascade-soft-deletes": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "lucascudo/laravel-pt-br-localization": "^1.1",
        "mll-lab/graphql-php-scalars": "^5.3",
        "mll-lab/laravel-graphql-playground": "^2.5",
        "nuwave/lighthouse": "^5.71",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-backup": "^6.16",
        "symfony/property-access": "^5.4"
    },
    "require-dev": {
        "bluemmb/faker-picsum-photos-provider": "^2.0",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.19",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
